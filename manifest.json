{
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "description": "Chromium search tools in the Omnibox",
  "icons": {
    "16": "assets/chromium_logo_16.png",
    "48": "assets/chromium_logo_48.png",
    "128": "assets/chromium_logo_128.png"
  },
  "manifest_version": 3,
  "name": "Omnibox for Chromium Developers",
  "omnibox": {
    "keyword": "cr"
  },
  "host_permissions": [
    "https://chromium-review.googlesource.com/*",
    "https://cs.chromium.org/codesearch/json/*",
    "https://source.chromium.org/*"
  ],
  "permissions": [
    "storage"
  ],
  "version": "1",
  "action": {
    "default_icon": {
      "16": "assets/chromium_logo_16.png",
      "48": "assets/chromium_logo_48.png",
      "128": "assets/chromium_logo_128.png"
    }
  }
}
